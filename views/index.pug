extends layout 

block layout-content
    div.TitleView
        h1 Personal Tracker
        div.MainView
            form(id="main-form" method="POST" action="/personal-tracker")
                select(name="select")
                    option(value="1") 1
                    option(value="2") 2
                    option(value="3") 3
                    option(value="4") 4
                    option(value="5") 5
                    option(value="6") 6
                    option(value="7") 7
                    option(value="8") 8
                button.btn-primary(type="submit") Submit